@model List<Result>

<div class="text-center mt-5">

@using (Html.BeginForm("ImageSearchResults", "ImageSearch", FormMethod.Post))
{
    <input id="searchTerm" name="searchTerm" placeholder="search..." type="text" />
    <button id="botaoEnviar" type="submit">Enviar</button>
}
</div>
<div class="mt-5">

@{
    @foreach (var Image in @Model)
    {
        <img src="@Image.urls.small" width=19% height=19% class="mt-1 mb-1 ms-1 me-1"/>
    }

}
</div>

